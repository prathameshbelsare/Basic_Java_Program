	// cal total and sum of marks
		int a,b,c,d,e;
		System.out.println("Enter marks of sub ");
		Scanner sc = new Scanner(System.in);
		a=sc.nextInt();
		b=sc.nextInt();
		c=sc.nextInt();
		d=sc.nextInt();
		e=sc.nextInt();
		
		int sum = a+b+c+d+e;
		System.out.println("total marks "+sum);
		double avg = sum/5;
		System.out.println("total avg "+avg);

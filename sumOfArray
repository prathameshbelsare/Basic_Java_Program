//Sum of Arrays 1st way
		int a[]= new int[5];
		int sum=0;
		
	    a[0]=10;
		a[1]=20;
		a[2]=30;
		a[3]=40;
		a[4]=50;
		for(int i=0; i<a.length; i++) {
			sum = a[i]+sum;
		}
		System.out.println(sum);
		
		// 2nd Way
		
		int b[]= {1,2,3,4,5,6,7,8,9};
		int sum1=0;
		for(int i=0; i<b.length; i++) {
			sum1 = b[i]+sum1;
		}
		System.out.println(sum1);

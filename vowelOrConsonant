//Vowel or consonant in java
	    char ch;
	    Scanner sc = new Scanner(System.in);
		System.out.println("plz Enter Character ");
		ch=sc.next().charAt(0);
		if(ch=='a' || ch=='e' || ch =='i' || ch=='o' || ch=='u') {
			System.out.println("   Vowel  ");
		}else {
			System.out.println("  consonant ");
		}

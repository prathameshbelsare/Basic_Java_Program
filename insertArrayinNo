//insert Array in no
		
		int arr[] = {1,2,3,4,5,6};  
	 
		int newArr[] = new int[arr.length+1];  
		int value = 7;  
		System.out.println(Arrays.toString(arr));  
		for(int i = 0; i<arr.length; i++) {  
			newArr[i] = arr[i];  
			}  
		newArr[arr.length] = value;  
		System.out.println(Arrays.toString(newArr)); 
